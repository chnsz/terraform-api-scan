{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_vpc_eip": {
          "block": {
            "attributes": {
              "address": {
                "type": "string",
                "computed": true
              },
              "auto_renew": {
                "type": "string",
                "optional": true,
                "forcenew": true
              },
              "charging_mode": {
                "type": "string",
                "optional": true,
                "computed": true,
                "forcenew": true
              },
              "enterprise_project_id": {
                "type": "string",
                "optional": true,
                "computed": true,
                "forcenew": true
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "ipv6_address": {
                "type": "string",
                "computed": true
              },
              "name": {
                "type": "string",
                "optional": true
              },
              "period": {
                "type": "number",
                "optional": true,
                "forcenew": true
              },
              "period_unit": {
                "type": "string",
                "optional": true,
                "forcenew": true
              },
              "port_id": {
                "type": "string",
                "computed": true
              },
              "private_ip": {
                "type": "string",
                "computed": true
              },
              "region": {
                "type": "string",
                "computed": true
              },
              "status": {
                "type": "string",
                "computed": true
              },
              "tags": {
                "type": [
                  "map",
                  "string"
                ],
                "optional": true
              }
            },
            "block_types": {
              "bandwidth": {
                "block": {
                  "attributes": {
                    "charge_mode": {
                      "type": "string",
                      "optional": true,
                      "computed": true,
                      "forcenew": true
                    },
                    "id": {
                      "type": "string",
                      "optional": true,
                      "computed": true,
                      "forcenew": true
                    },
                    "name": {
                      "type": "string",
                      "optional": true,
                      "computed": true
                    },
                    "share_type": {
                      "type": "string",
                      "required": true,
                      "forcenew": true
                    },
                    "size": {
                      "type": "number",
                      "optional": true,
                      "computed": true
                    }
                  }
                },
                "nesting_mode": "list",
                "min_items": 1,
                "max_items": 1
              },
              "publicip": {
                "block": {
                  "attributes": {
                    "ip_address": {
                      "type": "string",
                      "optional": true,
                      "computed": true,
                      "forcenew": true
                    },
                    "ip_version": {
                      "type": "number",
                      "optional": true,
                      "computed": true
                    },
                    "type": {
                      "type": "string",
                      "optional": true,
                      "forcenew": true,
                      "default": "5_bgp"
                    }
                  }
                },
                "nesting_mode": "list",
                "min_items": 1,
                "max_items": 1
              }
            }
          },
          "product": {
            "catalog": "Network",
            "name": "Elastic IP",
            "short": "EIP"
          }
        }
      }
    }
  }
}
